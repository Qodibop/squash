<h2>Signup to play</h2>


<form method="POST" action="/signupUser"  enctype='multipart/form-data'>
  <label>
    Username:
    <input type="text" name="username">
  </label>
  <br>
  <label>
    Password:
    <input type="password" name="password">
  </label>
  <br>
  <label>
  Photo de profil:
  <input type="file" name="user-picture">
</label>
<br>
  <label>
  email:
  <input type="text" name="email">
</label>
<br>
<label>
adresse:
<input type="text" name="address" id="address">
</label>
  <button>Signup</button>
  <label id="tohide" for="zipcode">Code postal :
    <input type="text" id="zipcode" name="zipcode">
  </label>
</form>

<hr>
<div class="text-center">
<h4>Already registered?</h4>
<a href="/loginUser"><button class="btn btn-primary btn-lg">Login</button></a>
</div>

