<section id="sectionEditUserProfile" class="p-3 m-3">

<h2 class="text-center">Edit your Profile</h2>
<hr class="mx-5 px-5">

<form method="POST" action="/editUserProfile"  enctype='multipart/form-data'>
<fieldset>
<div class="form-group">
    <label for="exampleInputUsername">Username</label>
    <input type="text" name="username" class="form-control" id="exampleInputUsername" placeholder="Enter your username">
  </div>

<div class="form-group">
  <label for="exampleInputPassword1">Password</label>
  <input type="password" name="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
</div>
  
    <div class="form-group">
      <label for="exampleInputEmail1">Email address</label>
      <input type="email" name="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
      <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
    </div>

    <div class="form-group">
    <label for="exampleInputAddress">Address</label>
    <input type="text" name="address" id="address" class="form-control" placeholder="Enter address">
  </div>
  
    <div class="form-group">
      <label for="profilePictureUser">Profile picture</label>
      <input type="file" name="user-picture" class="form-control-file" id="profilePictureUser" aria-describedby="fileHelp">
      <small id="fileHelp" class="form-text text-muted">Please select a profile picture, you can add it or modify it after signing up.</small>
    </div>
    
    <div class="form-group">
      <label for="exampleSelect1">Ranking</label>
      <select type="text" name="ranking" class="form-control" id="sectionEditRanking">
        <option value="Shitty">Shitty</option>
        <option value="Crapy">Crapy</option>
        <option value="I am Bon, super Bon">I am Bon, super Bon</option>
        <option value="Spaaaaartaaaaa!!">Spaaaaartaaaaa!!</option>
        <option value="I am a God">I am a God!</option>
      </select>
    </div>

  <div class="form-group">
      <label for="exampleTextarea">Modo</label>
      <textarea type="text" name="modo" class="form-control" id="modoArea" rows="1"></textarea>
    </div>
  
    <div class="form-group">
      <label for="exampleTextarea">Bio</label>
      <textarea type="text" name="info" class="form-control" id="bioArea" rows="3"></textarea>
    </div>
    
    <fieldset class="form-group">
      <div class="form-check">
        <label class="form-check-label">
          <input class="form-check-input" type="checkbox" value="" checked="">
          I agree with the terms and conditions of Asphalt Club.
        </label>
      </div>
    </fieldset>

    <button type="submit" class="btn btn-primary">Save changes</button>
    <label id="tohide" for="zipcode">Code postal :
      <input type="text" id="zipcode" name="zipcode">
    </label>
  </fieldset>
</form>

</section>

